  "name": "Serika",
  "tokenColors": [
    {
      "scope": ["variable"],
      "settings": {
        "foreground": "#d1d0c5"
      }
    },
    {
      "scope": [
        "keyword.operator.assignment",
        "brace",
        "bracket",
        "delimiter",
        "meta",
        "paren",
        "tag"
      ],
      "settings": {
        "foreground": "#737579"
      }
    },
    {
      "scope": ["storage"],
      "settings": {
        "foreground": "#737579"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#d1d0c5"
      }
    },
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#737579"
      }
    },
    {
      "scope": ["constant", "entity.name"],
      "settings": {
        "foreground": "#FFCB00"
      }
    },
    {
      "scope": ["constant"],
      "settings": {
        "fontStyle": "",
        "foreground": "#ea4a5a"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#ea4a5a"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#B267E6"
      }
    }
  ],
  "type": "dark"
}
